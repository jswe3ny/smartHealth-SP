{
  "expo": {
    "name": "smartHealth-SP",
    "slug": "smartHealth-SP",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "smarthealthsp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.anonymous.smartHealth-SP",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan product barcodes."
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.anonymous.smartHealthSP",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["CAMERA"]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "icon": "./assets/images/react-logo@2x.png", 
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#26b521"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to scan barcodes."
        }
      ]
    ],
    
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d7206bb3-ac28-460f-8738-15f6f56dd4cf"
      }
    }
  }
}
